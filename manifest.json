{
	"name": "Output",
	"description": "Templating engine with cache management (install matthiasmullie/minify for minify features)",
	"version": "0.7.0",
	"dependencies": {
		"Core": ">=3.0.0"
	},
	"file-types": {
		"template": {
			"folder": "templates",
			"class": false
		}
	},
	"bind-methods": {
		"addCSS": "addCSS",
		"addJS": "addJS",
		"removeCSS": "removeCSS",
		"removeJS": "removeJS",
		"wipeCSS": "wipeCSS",
		"wipeJS": "wipeJS",
		"getCSSList": "getCSSList",
		"getJSList": "getJSList",
		"sendJSON": "sendJSON",
		"inject": "inject",
		"injected": "injected"
	},
	"requires": [
		"model/assets"
	]
}
